package sintaxeVariaveisFluxos;

public class Testlacos2 {

	public static void main(String[] args) {
		
		//i = linha
		for (int i = 1; i <= 10; i++) {
			// j = coluna
			for(int j = 1; j<=10; j++) {
				if (j >= i) {
					break;
				}
				System.out.print("*");
				
			}
			System.out.println();
		}
	}
}
